{
  "version": 2,
  "buildCommand": "cd web && npm run build",
  "outputDirectory": "web/dist",
  "installCommand": "npm install && cd web && npm install",
  "framework": "vite",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://your-backend-url.railway.app/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
